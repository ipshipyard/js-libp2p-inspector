{
  "name": "@ipshipyard/libp2p-inspector",
  "version": "1.0.0",
  "description": "Inspect a running libp2p node",
  "type": "module",
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "build": "tsc && node esbuild.js",
    "test": "aegir test -t node",
    "test:electron-main": "aegir test -t node",
    "dep-check": "aegir dep-check",
    "start": "npm run build && electron --trace-warnings --unhandled-rejections=strict ./dist/src/main/index.js"
  },
  "repository": "https://github.com/ipshipyard/js-libp2p-inspector",
  "keywords": [
    "libp2p"
  ],
  "author": "info@ipshipyard.com",
  "license": "Apache-2.0 OR MIT",
  "dependencies": {
    "@ipshipyard/libp2p-inspector-metrics": "^0.0.0",
    "@ipshipyard/libp2p-inspector-ui": "^0.0.0",
    "@libp2p/interface": "^2.9.0",
    "@libp2p/peer-id": "^5.1.2",
    "@libp2p/utils": "^6.6.4",
    "@multiformats/multiaddr": "^12.4.0",
    "@types/multicast-dns": "^7.2.4",
    "delay": "^6.0.0",
    "electron": "^36.2.1",
    "it-length-prefixed": "^10.0.1",
    "it-pipe": "^3.0.1",
    "it-pushable": "^3.2.3",
    "it-rpc": "^1.0.4",
    "multicast-dns": "^7.2.5",
    "multiformats": "^13.3.6",
    "race-signal": "^1.1.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "stream-to-it": "^1.0.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@libp2p/webrtc": "^5.2.14",
    "@playwright/test": "^1.52.0",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "aegir": "^47.0.12",
    "libp2p": "^2.8.7",
    "playwright-core": "^1.52.0"
  }
}
