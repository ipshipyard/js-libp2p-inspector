{
  "name": "@ipshipyard/libp2p-inspector-metrics",
  "version": "0.0.0",
  "description": "Collect libp2p metrics and send them to browser DevTools",
  "author": "info@ipshipyard.com",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/ipshipyard/js-libp2p-inspector/tree/main/packages/libp2p-inspector-metrics#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipshipyard/js-libp2p-inspector.git"
  },
  "bugs": {
    "url": "https://github.com/ipshipyard/js-libp2p-inspector/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build",
    "test": "aegir test -t browser",
    "test:chrome": "aegir test -t browser --cov",
    "test:firefox": "aegir test -t browser --browser firefox"
  },
  "dependencies": {
    "@libp2p/interface": "^2.9.0",
    "@libp2p/interface-internal": "^2.3.11",
    "@libp2p/logger": "^5.1.15",
    "@libp2p/peer-id": "^5.1.2",
    "@libp2p/simple-metrics": "^1.3.9",
    "@libp2p/utils": "^6.6.4",
    "@multiformats/multiaddr": "^12.3.3",
    "cborg": "^4.2.6",
    "dns-packet": "^5.6.1",
    "it-length-prefixed": "^10.0.1",
    "it-pipe": "^3.0.1",
    "it-pushable": "^3.2.3",
    "it-rpc": "^1.0.2",
    "multicast-dns": "^7.2.5",
    "multiformats": "^13.3.1",
    "progress-events": "^1.0.1",
    "stream-to-it": "^1.0.1",
    "uint8arrays": "^5.1.0"
  },
  "devDependencies": {
    "@libp2p/crypto": "^5.1.1",
    "aegir": "^47.0.12",
    "sinon-ts": "^2.0.0"
  },
  "browser": {
    "./src/messages/messages.js": "./src/messages/messages.browser.js"
  },
  "sideEffects": false
}
